  "main": "./build/index.cjs",
  "module": "./build/index.js",
  "types": "./build/index.d.ts",
  "type": "module",
  "private": false,
  "files": [
    "build"
  ],
  "exports": {
    ".": {
      "types": "./build/index.d.ts",
      "import": "./build/index.js",
      "require": "./build/index.cjs"
    }
  },
  "scripts": {
    "clean": "rm -rf build",
    "build": "npm run clean && tsup src/index.ts --outDir build --format esm,cjs --dts",
    "test": "vitest --config vitest.config.mts"
  },
  "dependencies": {
    "@fastify/cors": "^10.0.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.2",
    "fastify": "^5.0.0",
    "reflect-metadata": "^0.2.2",
    "unplugin-swc": "^1.5.1",
    "winston": "^3.15.0"
  },
  "devDependencies": {
    "@swc/core": "^1.9.2",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/express": "^5.0.0",
    "@types/node": "^22.5.5",
    "@types/supertest": "^6.0.2",
    "@types/winston": "^2.4.4",
    "prettier": "^3.3.3",
    "supertest": "^7.0.0",
    "ts-node-dev": "^2.0.0",
    "tsup": "^8.3.5",
    "typescript": "^5.6.2",
    "vitest": "^2.1.1"
  }
}
